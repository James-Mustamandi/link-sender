{
  "manifest_version": 3,
  "name": "Send Email",
  "version": "1.0",
    
  "permissions": [
    "*://api.emailjs.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },

  "description": "Send emails from a popup.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  }

  

}
